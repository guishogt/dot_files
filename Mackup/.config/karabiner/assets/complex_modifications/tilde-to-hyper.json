{
    "title": "Change Insert (rev 5)",
    "rules": [
      {
        "description": "Change insert key to command+control+option+shift if pressed with other keys",
        "manipulators": [
          {
            "type": "basic",
            "from": {
              "key_code": "insert",
              "modifiers": {
                "optional": [
                  "any"
                ]
              }
            },
            "to": [
              {
                "key_code": "left_shift",
                "modifiers": [
                  "left_command",
                  "left_control",
                  "left_option"
                ]
              }
            ],
            "to_if_alone": [
              {
                "hold_down_milliseconds": 100,
                "key_code": "insert"
              }
            ]
          }
        ]
      },
      {
        "description": "Change insert key to command+control+option+shift. (Post escape key when pressed alone)",
        "manipulators": [
          {
            "type": "basic",
            "from": {
              "key_code": "insert",
              "modifiers": {
                "optional": [
                  "any"
                ]
              }
            },
            "to": [
              {
                "key_code": "left_shift",
                "modifiers": [
                  "left_command",
                  "left_control",
                  "left_option"
                ]
              }
            ],
            "to_if_alone": [
              {
                "key_code": "escape"
              }
            ]
          }
        ]
      },
      {
        "description": "Change insert key to command+control+option+shift. (Post f19 key when pressed alone)",
        "manipulators": [
          {
            "type": "basic",
            "from": {
              "key_code": "insert",
              "modifiers": {
                "optional": [
                  "any"
                ]
              }
            },
            "to": [
              {
                "key_code": "left_shift",
                "modifiers": [
                  "left_command",
                  "left_control",
                  "left_option"
                ]
              }
            ],
            "to_if_alone": [
              {
                "key_code": "f19"
              }
            ]
          }
        ]
      },
      {
        "description": "Change insert key to command+control+option+shift. (Use shift+insert as insert)",
        "manipulators": [
          {
            "type": "basic",
            "from": {
              "key_code": "insert",
              "modifiers": {
                "mandatory": [
                  "shift"
                ],
                "optional": [
                  "insert"
                ]
              }
            },
            "to": [
              {
                "key_code": "insert"
              }
            ]
          },
          {
            "type": "basic",
            "from": {
              "key_code": "insert",
              "modifiers": {
                "optional": [
                  "any"
                ]
              }
            },
            "to": [
              {
                "key_code": "left_shift",
                "modifiers": [
                  "left_command",
                  "left_control",
                  "left_option"
                ]
              }
            ]
          }
        ]
      },
      {
        "description": "Change insert to control if pressed with other keys, to escape if pressed alone.",
        "manipulators": [
          {
            "type": "basic",
            "from": {
              "key_code": "insert",
              "modifiers": {
                "optional": [
                  "any"
                ]
              }
            },
            "to": [
              {
                "key_code": "left_control"
              }
            ],
            "to_if_alone": [
              {
                "key_code": "escape"
              }
            ]
          }
        ]
      },
      {
        "description": "Change insert to control if pressed with other keys. (rev 2)",
        "manipulators": [
          {
            "type": "basic",
            "from": {
              "key_code": "insert",
              "modifiers": {
                "optional": [
                  "any"
                ]
              }
            },
            "to": [
              {
                "key_code": "left_control"
              }
            ],
            "to_if_alone": [
              {
                "key_code": "insert",
                "hold_down_milliseconds": 500
              }
            ]
          }
        ]
      }
    ]
  }